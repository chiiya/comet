{
  "name": "@comet-cli/core",
  "version": "0.1.0",
  "description": "Core comet cli application",
  "keywords": [
    "comet",
    "cli",
    "api",
    "documentation",
    "testing"
  ],
  "author": "Elisha Witte <elishawitte@gmail.com>",
  "homepage": "https://github.com/chiiya/comet/tree/master/packages/core#readme",
  "license": "MIT",
  "bin": {
    "comet": "./bin/run"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "comet",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiiya/comet.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc": "tsc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": {
    "url": "https://github.com/chiiya/comet/issues"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "assign-deep": "^1.0.0",
    "chalk": "^2.4.2",
    "cosmiconfig": "^5.0.7",
    "dot-prop": "^4.2.0",
    "fs-extra": "^7.0.1",
    "ora": "^3.0.0",
    "signale": "^1.3.0",
    "swagger-parser": "^6.0.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/chai": "^4",
    "@types/cosmiconfig": "^5.0.3",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/ora": "^1.3.4",
    "@types/signale": "^1.2.0",
    "globby": "^8",
    "ts-node": "^7",
    "tslint": "^5",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.0"
  },
  "gitHead": "8b3651f7766bb0f7f92ce2675f649f9a51c78b2c"
}
